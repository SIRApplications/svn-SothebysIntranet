/* Programmer Code *//* End Programmer Code */html {	height: 100%;}body {	height: 100%;	margin: 0;	font: 12px/18px Arial, sans-serif;	color: #333333;}a {	color: #767678;}a:hover {	color: #1F5A89;}.alternate {	background: #E5EBF3;}button {	float: right;	padding: 1px 2px;	border: 0;	font: 12px/18px Arial, sans-serif;	color: #FFFFFF;	cursor: pointer;	background-color: #11568B;	height: 18px;	line-height: 16px;	margin: 0 30px 0 0;}.danger {	color: #FF0000;}em {	font: italic 10px/12px Arial, sans-serif;	color: #999999;	margin: 0 0 10px 0;}.error {	color: #11568B;}form {	margin: 0;}h1 {	float: left;	display: block;	width: 201px;	height: 94px;	margin: 0;	padding: 0;	background: url('../images/logo.gif') no-repeat;;}h1 a {	display: block;	height: 100%;	width: 100%;}h2 {	margin: 0 12px 8px 0;	padding: 8px 0 0 0;	font: normal 20px/16px Arial, sans-serif;	text-align: right;}.content h3 {	margin: 0;	font: normal 18px/16px Arial, sans-serif;	color: #FFFFFF;	padding: 6px 26px;	background-color: #1F5A89;	margin: 0 0 14px 0;}.content table h3 {	font: normal 18px/16px Arial, sans-serif;	color: #FFFFFF;	background-color: transparent;	padding: 0;	margin: 0 0 2px 0;}h4 {	margin: 0 0 0 26px;	font-size: 14px;	font-weight: normal;	color: #1F5A89;}h5 {	margin: 0 0 0 26px;	font-size: 12px;	color: #1F5A89;}img {	border: 0;}input.text {	float: right;	font-size: 10px;	padding: 1px 2px;	border: 1px solid #D9D9D9;	width: 124px;	margin: 0 10px 0 0;}label {	float: right;	font-size: 11px;	margin: 0 6px 0 0;}li {	list-style: none;}.content li {	list-style: disc;}.new {	color: #FF0000;	font-style: italic;	font-size: 10px;}p {	margin: 0 0 15px 26px;	padding: 0;}table p {	margin: 0;}.registered {	font-size: 9px;}span.alt {	display: none;}ul {	margin: 0;	padding: 0;}.content ul {	margin: 0 0 10px 56px;}.container {	position: relative;	width: 972px;	height: 100%;	margin: 0 auto;}.header {	background: #000000 url('../images/bg-header.gif') bottom no-repeat;	color: #FFFFFF;	height: 135px;}.header a {	color: #FFFFFF;}/* login */.header .login {	float: right;	text-align: right;	width: 741px;	padding: 49px 30px 0 0;	font-size: 11px;}.header .login a:hover {	color: #999999;}.main .login a {	color: #1F5A89;}.main .login a:hover {	color: #767678;}/* end login */.quickSearch {	float: right;	height: 26px;	width: 741px;	padding: 10px 0 0 0;}/* top navigation */.nav {	height: 25px;	width: 972px;	float: none;	clear: both;}.nav ul {	background: #11568B;	width: 972px;	height: 25px;}.nav li {	float: left;	margin: 0;	height: 25px;}.nav li a {	height: 100%;	width: 100%;	display: block;}/* save for top nav */.nav .item {	width: 170px;	background: url('../images/nav-item.gif') no-repeat;}.nav .item a:hover {	background: url('../images/nav-propertiesForSale-r.gif') no-repeat;}.item .item a {	background: url('../images/nav-propertiesForSale-r.gif') no-repeat;}/* top navigation */.main {	clear: both;	margin: 10px auto 16px auto;	width: 918px;	display: table;	background: url('../images/bg-main.gif') bottom right no-repeat;}/* sideBar */.sideBar {	float: left;	width: 210px;}.sideBar .header {	padding: 12px 18px 12px 26px;	color: #333333;	width: 166px;	background: none;	height: auto;}/* filters definition list */.sideBar dl {	background: url('../images/bg-filters.gif') bottom no-repeat;	margin: 0 0 22px 0;	padding: 6px 4px;	color: #767678;}.sideBar dt {	border-top: 1px solid #FFFFFF;	width: 202px;	margin: 0;}.sideBar dt.first {	border: 0;}.sideBar dt a {	display: block;	color: #666666;	text-decoration: none;	padding: 1px 10px 2px 22px;	width: 170px;}.sideBar dt a:hover {	background: url('../images/bg-filters-on.gif') bottom no-repeat;}.sideBar dt.activated a,.sideBar dt.activated a:hover {	color: #FFFFFF;	background: url('../images/bg-filters-activated.gif') bottom no-repeat;}.sideBar dt.selected a,.sideBar dt.selected a:hover {	color: #FFFFFF;	background: url('../images/bg-filters-selected.gif') bottom no-repeat;}.sideBar dd {	margin: 0;	padding: 8px 10px 8px 22px;	background: url('../images/bg-dd.gif') no-repeat;}.sideBar dd a {	text-decoration: none;}.sideBar dd a.on {	color: #1F5A89;}/* end filters definition list */.announcement {	height: 63px;	width: 210px;	background: url('../images/bg-announcement.gif') no-repeat;	margin: 0 0 5px 0;	padding: 14px 0 0 0;	font-weight: bold;	text-align: center;}.quickLinks a {	margin: 0 0 0 26px;}hr {	margin: 0 0 10px 0;	padding: 0;	border: 0;	border-bottom: 1px solid #DDDDDD;	height: 1px;}/* end sideBar */.content {	float: right;	width: 680px;	margin: 0 0 0 26px;	padding: 160px 0 0 0;	position: relative;	z-index: 0;	background: url('../images/bg-content.jpg') no-repeat;}#marketing .content {	background: url('../images/bg-content-marketing.jpg') no-repeat;}.subheader {	padding-top: 40px;	width: 100%;	float: left;}table {	width: 100%;	color: #FFFFFF;	margin: 0 0 4px 0;	border-collapse: collapse;}table td {	padding: 26px;	vertical-align: top;}table a {	color: #FFFFFF;}table a:hover {	color: #1F5A89;}.topLeft {	width: 427px;	background-color: #DFD186;	border-right: 4px solid #FFFFFF;}.topRight {	width: 250px;	background-color: #7FA399;}.bottomRight {	width: 427px;	background-color: #4E5A5A;	border-left: 4px solid #FFFFFF;}.bottomLeft {	width: 250px;	background-color: #567969;}table td img {	border: 1px solid #FFFFFF;	float: right;	margin: 6px 0 0 0;}/* footer */.footer {	background: #000000 url('../images/bg-footer.gif') center left no-repeat;	margin: 0 0 10px 0;	padding: 20px 26px 20px 64px;	font-size: 10px;	line-height: 14px;	color: #FFFFFF;}.footer a {	color: #FFFFFF;}.disclaimer {	font-size: 10px;}.footerNavigation {	font-size: 10px;	line-height: 14px;	text-align: center;	padding: 0 0 15px 265px;}/* end footer */